boolean permutation(String s, String t) {
	if (s.length() != t.length()) {
		return false;
	}

	int[] charArr = new int[128];
	char[] sArr = s.toCharArray();
	for (char c : sArr) {
		charArr[c]++;
	}

	for(int i = 0; i < t.length(); i++) {
		int c = (int) t.charAr(i);
		charArr[c]--;
		if (charArr[c] < 0) {
			return false;
		}
	}
	return true;

}